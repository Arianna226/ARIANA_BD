CREATE DATABASE ARIANA_BD;
USE ARIANA_BD;
CREATE TABLE ESTUDIANTE(
id int primary key,
nombre varchar(50),
F_D_N date
)
select * from ESTUDIANTE;
INSERT INTO ESTUDIANTE (id,  nombre, F_D_N)
VALUES (1, 'TOM', '2004-10-7'), (2, 'TOMAS', '2004-10-7');

DELETE FROM ESTUDIANTE WHERE id=2;

CREATE TABLE MATERIAS (
CODIGO INT PRIMARY KEY IDENTITY(1,1),
NOMBRE VARCHAR (50)
);

INSERT INTO MATERIAS(NOMBRE) VALUES('INGLES');

SELECT * FROM MATERIAS;

CREATE TABLE INSCRIPCIONES (
ESTUDIANTEID INT,
MATERIACODIGO INT,
FOREIGN KEY (ESTUDIANTEID) REFERENCES ESTUDIANTE (ID),
FOREIGN KEY (MATERIACODIGO) REFERENCES MATERIAS (CODIGO)
);

SELECT * FROM INSCRIPCIONES;
INSERT INTO INSCRIPCIONES (ESTUDIANTEID, MATERIACODIGO)
VALUES (1,2);
